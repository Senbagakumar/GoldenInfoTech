@model IEnumerable<SelfAssessment.Models.UIGroup>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="content mt-3">
    <div class="col-md-12" id="divGroupAddEditForm" style="display:none">
