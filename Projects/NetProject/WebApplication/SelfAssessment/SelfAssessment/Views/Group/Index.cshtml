                        :  <label id="lbl-GD-text-input" />
                    </div>
                </div>
                <div class="row">&nbsp;</div>

                <table id="questiontable" class="table table-striped table-bordered">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Question Code</th>
                            <th>Question</th>
                            <th>User Hint</th>
                            <th>Timer Limit</th>
                            <th>Options</th>
