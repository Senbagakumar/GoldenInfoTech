            <div class="col-md-7">
                <div class="stl-map-ctnr afb-clear">
                    <iframe height="350" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?f=d&amp;source=s_d&amp;saddr=18109+NE+76th+St.+Suite+108,+Redmond,+WA+98052&amp;daddr=&amp;hl=en&amp;geocode=&amp;aq=&amp;sll=31.168934,-100.076842&amp;sspn=7.910618,13.249512&amp;mra=ls&amp;ie=UTF8&amp;t=m&amp;ll=47.671594,-122.098248&amp;spn=0.006295,0.009349&amp;output=embed"></iframe>
                </div>
            </div>

        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        $("li").removeClass("active");
        $("a:contains('Events')").parent().addClass("active");
        stlCustomTable();
    })
    

</script>

