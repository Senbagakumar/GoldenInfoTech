@model System.Data.DataTable
@using System.Data;

@{
    ViewBag.Title = "events";
}

<script type="text/javascript" src="https://addevent.com/libs/atc/1.6.1/atc.min.js" async defer></script>
<!-- Events Page Content -->
<div class="stl-glb-ctnr afb-clear stl-ctnr-three" id="events">
    <h1>Events</h1>
    <hr class="stl-hr" />

    <div class="stl-tbl-ctnr afb-clear">
        <!--
        <iframe src="https://calendar.google.com/calendar/embed?showTitle=0&amp;showPrint=0&amp;showTz=0&amp;height=600&amp;wkst=1&amp;bgcolor=%23FFFFFF&amp;src=bvehlc8l4t1e7pn607nskb0ab4%40group.calendar.google.com&amp;color=%23d2310b&amp;ctz=Asia%2FCalcutta" style="border-width:0" width="100%" height="600" frameborder="0"></iframe>
    -->
        <div class="stl-evnt-ctnr">

            @foreach (DataRow row in Model.Rows)
            {
                <div class="well stl-well clearfix">

                    @foreach (DataColumn col in Model.Columns)
                    {
                        <div class="col-sm-6 stl-custum-td">
                            <p>@row[col.ColumnName]</p>
                        </div>
                    }

                </div>
            }
        </div>
    </div>
</div>
<div class="stl-glb-ctnr afb-clear ">
    
    <div class="stl-ctnr-one stl-glb-ctnr">
        <div class="stl-glb-ctnt stl-ctnt-one">
            <ul class="stl-ul-ctnr gggb-ul">
                <li><b><i>Every Sunday 11:00AM to 12:30PM Gajanan Maharaj Puja</i></b></li>
                <li>Please contact temple office for Abhishekam, Baba Dress, Garland, Annadhanam sponsorship</li>
                <li>Contact temple office to participating in any of these Vrathams/Pujas</li>
            </ul>
        </div>
    </div>
</div>
<div class="stl-glb-ctnr afb-clear ">
    <h1>Temple Address :</h1>
    <hr class="stl-hr" />
    <div class="stl-glb-ctnr stl-ctnr-one">
        <div class="stl-glb-ctnt">
            <div class="col-md-5">
                <div class="stl-adrs-ctnr afb-clear">
                    <address class="stl-adrs-ctnt" style="height: 358px;">
                        Address:
                        Shirdi Sai Baba Temple of Seattle
                        18109 NE 76th St, Suite #108
                        Redmond, WA - 98052
                        <div class="stl-adrs-btn-ctnr">
                            <a href="https://www.facebook.com/SaibabaSeattle" target="_blank" class="sr-icon btn stl-btn stl-fb" id="fb"> <i class="ion ion-social-facebook"></i></a>
                            <a href="mailto:admin@saibabaseattle.com" target="_top" class="sr-icon btn stl-btn stl-fb" id="mail-i"> <i class="ion ion-android-mail"></i></a>
                            <a href="https://www.youtube.com/channel/UCCtB1YmyTFIX5tQbqGfT3AQ/featured" target="_blank" class="sr-icon btn stl-btn stl-fb" id="yt"><i class="ion ion-social-youtube"></i></a>

                            <p><span class="stl-phne-icnctnr"><i class="ion ion-ios-telephone"></i></span>(425) 999-6350.</p>
