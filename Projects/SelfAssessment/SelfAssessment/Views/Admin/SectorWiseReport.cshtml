
@{
    ViewBag.Title = "SectorWieReport";
}

<h2>SectorWieReport</h2>

<div class="content mt-3">


    <div class="col-sm-12">
        <div class="card">
            <div class="card-header">
                <strong>Sector Wise Report</strong>
                <div class="user-area dropdown float-right">

                    <a href="#" class="dropdown-toggle btn btn-primary" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span class="ti-export"></span>&nbsp; Export
                    </a>

                    <div class="user-menu dropdown-menu">
                        <a class="nav-link" href="#"><i class="ti-file"></i> Export AS PDF</a>
                        <a class="nav-link" href="#"><i class="ti-microsoft"></i> Export AS Excel</a>
                    </div>
                </div>
            </div>
            <div class="card-body card-block">
                <form action="" method="post" class="form-horizontal">
                    <div class="row form-group">
                        <div class="col col-md-1"><label for="hf-password" class=" form-control-label">Assessment </label></div>
                        <div class="col-12 col-md-3">
                            Display Assessment Name
                        </div>
                        <div class="col col-md-1"><label for="hf-password" class=" form-control-label">Levels</label></div>
                        <div class="col-12 col-md-3">
                            <select class="form-control">
                                <option value="0">-Select-</option>
                                <option value="survey 1" selected>Level 1</option>
                                <option value="survey 2">Level 2</option>
                                <option value="survey 3">Level 3</option>
                            </select>
                        </div>
                        <div class="col col-md-1"><label for="hf-password" class=" form-control-label">Sector</label></div>
                        <div class="col-12 col-md-3">
                            <select class="form-control">
                                <option value="0">-Select-</option>
                            </select>
                        </div>

                    </div>

                    <div class="row form-group">
                        <div class="col col-md-1"><label for="hf-password" class=" form-control-label">Sub Sector </label></div>
                        <div class="col-12 col-md-3">
                            <select class="form-control">
                                <option value="0">-Select-</option>
                                <option value="survey 1" selected>Level 1</option>
                                <option value="survey 2">Level 2</option>
                                <option value="survey 3">Level 3</option>
                            </select>
                        </div>


                        <div class="col-12 col-md-4">
                            <button type="submit" class="btn btn-primary btn-sm">
                                <i class="fa fa-dot-circle-o"></i> Go
                            </button>
                            <button type="reset" class="btn btn-danger btn-sm">
                                <i class="fa fa-ban"></i> Reset
                            </button>
                        </div>
                    </div>
            </div>
            </form>
        </div>

    </div>

    <!--/.col-->

    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="col-lg-6">
                    <div class="card">
                        <div class="card-body">
                            <h4 class="mb-3">Organization Score (%) Result </h4>
                            <canvas id="barChart1"></canvas>
                        </div>
                    </div>
                </div>



                <div class="col-lg-6">
                    <div class="card">
                        <div class="card-body">
                            <h4 class="mb-3">Final Score (%) Result </h4>
                            <canvas id="barChart3"></canvas>
                        </div>
                    </div>
                </div>

                <table id="bootstrap-data-table" class="table table-striped table-bordered">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Manufacture</th>
                            <th>City</th>
                            <th>State</th>
                            <th>Type of Services</th>
                            <th>Sector</th>
                            <th>Sub Sector</th>
                            <th>Revenue</th>
                            <th>Option</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>1</td>
                            <td><a href="customerDetails.html">Name</a></td>
                            <td>Madurai</td>
                            <td>Tamilnadu</td>
                            <td>Services</td>
                            <td>Sector</td>
                            <td>sub Sector</td>
                            <td>Revenue</td>
                            <td><a href="/Admin/customerDetails" class="btn btn-info btn-sm"> <i class="fa fa-eye"></i></a>
                        </tr>

                    </tbody>
                </table>


            </div>
        </div>

        <!-- .animated -->
    </div><!-- .content -->


</div><!-- /#right-panel -->
<!-- Right Panel -->

<script src="~/Admin/assets/js/vendor/jquery-2.1.4.min.js"></script>
<script src="~/Admin/assets/js/popper.min.js"></script>
<script src="~/Admin/assets/js/plugins.js"></script>
<script src="~/Admin/assets/js/main.js"></script>


<!--  Chart js -->
<script src="~/Admin/assets/js/lib/chart-js/Chart.bundle.js"></script>
<script src="~/Admin/assets/js/lib/chart-js/chartjs-init1.js"></script>


